{
     "version": 8,
     "tmdbApi": "c13bbebafbd3dad0e281c6241304aeff",
     "omdbApi": "",
     "agent": {
          "type": "emby",
          "url": "https://192.168.1.100:8096",
          "apiKey": ""
     },
     "scraping": {
          "RT": true,
          "IMDB": true,
          "LB": true,
          "TVTime": true,
          "MetaCritic": true,
          "Trakt": true,
          "RE": true
     },
     "defaultAudioLanguage": "ENG",
     "mediaInfoUpdateInterval": 30,
     "IMDBDatasetUpdateInterval": 7,
     "preferedImageLanguage": "en-US",
     "wkhtmltoimage": "/usr/bin/wkhtmltoimage",
     "ffprobe": "/usr/bin/ffprobe",
     "languagesOrder": ["SPA","ENG",  "JPN", "POR", "FRE", "ITA", "DAN", "KOR", "RUS", "SWE", "ZHO", "HIN", "AZE", "IND", "DUT", "POL"],
     "extensions": ["mkv", "mp4", "avi", "m2ts", "m4v"],
     "templates": [
          {
               "type": ["tv_backdrop", "movie_backdrop"],
               "template": "backdrop2.html",
               "out": ["backdrop.jpg", "thumb.jpg"]
          },
          {
               "type": ["tv", "movie", "season"],
               "template": "cover.html",
               "out": ["folder.jpg"]
          },
          {
               "type": ["episode_backdrop"],
               "template": "backdrop2.html",
               "out": ["$NAME.jpg"]
          }
     ]
}